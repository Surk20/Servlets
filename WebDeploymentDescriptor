        <url-pattern>/httpSession</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InterfaceSessionServlet</servlet-name>
        <servlet-class>InterfaceSessionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InterfaceSessionServlet</servlet-name>
        <url-pattern>/interfaceSession</url-pattern>
    </servlet-mapping>
